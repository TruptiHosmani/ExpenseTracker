
<label class="label"><%= t("activerecord.attributes.job_title.name", :default => t("activerecord.labels.name", :default => "Name")) %>:</label>
<p><%= @job_title.name %></p>

<div class="form-actions">
<%= link_to "Back", job_titles_path, :class => 'btn'  %>
<%= link_to "Edit", edit_job_title_path(@job_title), :class => 'btn' %>
<%= link_to "Delete", job_title_path(@job_title), :method => "delete", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => 'btn' %>
</div>
