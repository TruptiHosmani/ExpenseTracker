<h1>Job Titles</h1>
<table class="table table-striped">
 <thead>
  <tr>
    <th>ID</th>
        <th>
      <%= t("activerecord.attributes.job_title.name", :default => t("activerecord.labels.name", :default => "Name")) %>
    </th>
        <th>Created at</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @job_titles.each do |job_title| -%>
   <tr>
    <td><%= job_title.id %></td>
        <td><%= link_to job_title.name, job_title_path(job_title) %>
    </td>
        <td><%= job_title.created_at %></td>
    <td>
      <%= link_to "Show", job_title_path(job_title) %>
      <%= link_to "Edit", edit_job_title_path(job_title) %>
      <%= link_to "Destroy", job_title_path(job_title), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
    </td>
    </tr>
    <% end -%>
   </tbody>
</table>

<%= link_to "New", new_job_title_path, :class => 'btn btn-primary' %>
