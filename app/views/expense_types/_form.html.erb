
<div class="clearfix">
   <%= f.label :name, t("activerecord.attributes.expense_type.name", :default => "Name"), :class => :label %>
  <div class="input">
    <%= f.text_field :name, :class => 'text_field' %>
  </div>
</div>

<div class="clearfix">
   <%= f.label :max_expensable, t("activerecord.attributes.expense_type.max_expensable", :default => "Max expensable"), :class => :label %>
  <div class="input">
    <%= f.text_field :max_expensable, :class => 'text_field' %>
  </div>
</div>
<div class="form-actions">
  <button class="btn primary" type="submit">Save</button> or
	<%= link_to "Cancel", expense_types_path %>
</div>
